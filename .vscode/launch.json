    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Suitecloud setupaccoun",
        "program": "${workspaceFolder}/packages/node-cli/src/suitecloud.ts",
        "args": ["--help"],
        "outFiles": ["${workspaceFolder}packages/node-cli/dist/**/*.js"]
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Suitecloud suitecommerce:localserver",
        "program": "${workspaceFolder}/packages/node-cli/dist/suitecloud.ts",
        "protocol": "inspector",
        "args": ["suitecommerce:localserver, -i"],
        "cwd": "${workspaceFolder}/../../com.netsuite.testing",
        "outFiles": ["${workspaceFolder}packages/node-cli/dist/**/*.js"]
      }
    ]
  }