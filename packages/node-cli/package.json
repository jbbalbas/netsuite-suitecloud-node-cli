{
	"name": "@oracle/suitecloud-cli",
	"version": "1.0.2",
	"license": "UPL-1.0",
	"description": "SuiteCloud CLI for Node.js",
	"keywords": [
		"NetSuite",
		"SuiteCloud",
		"SDF"
	],
	"publishConfig": {
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/oracle/netsuite-suitecloud-sdk/issues"
	},
	"repository": "https://github.com/oracle/netsuite-suitecloud-sdk/tree/master/packages/node-cli",
	"main": "./src/suitecloud.js",
	"preferGlobal": true,
	"sdkDownloadUrl": "https://system.netsuite.com/download/suitecloud-sdk/cli/java/latest",
	"bin": {
		"scloud": "./dist/suitecloud.js"
	},
	"dependencies": {
		"@oracle/suitecloud-cli-localserver-command": "^1.0.2",
		"chalk": "^2.4.2",
		"cli-spinner": "^0.2.10",
		"commander": "^2.20.0",
		"inquirer": "^6.3.1",
		"request-promise-native": "^1.0.7",
		"vm2": "^3.8.0",
		"xml2js": "^0.4.19"
	},
	"devDependencies": {
		"@types/inquirer": "^6.5.0",
		"@types/jest": "^25.1.4",
		"@types/node": "^12.0.0",
		"copyfiles": "^2.2.0",
		"jest": "^24.7.1",
		"typescript": "^3.7.5"
	},
	"scripts": {
		"clean": "rm -rf dist && rm -rf tsconfig.build.tsbuildinfo",
		"copy-files": "copyfiles -u 1 \"src/**/*.{js,xml,template,json}\" dist",
		"build": "npm run clean && npm run copy-files && tsc -b tsconfig.build.json",
		"test": "jest",
		"postinstall": "node dist/postinstall.js"
	}
}
